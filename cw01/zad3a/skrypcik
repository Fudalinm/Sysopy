#!/bin/bash

		make static
		make shared
		
		./zad3_static 1000 1500 0 0 7000
		./zad3_static 1000 1500 1 0 7000
		./zad3_static 1000 1500 0 1 7000
		./zad3_static 1000 1500 1 1 7000
		./zad3_static 1000 1500 0 2 7000
		./zad3_static 1000 1500 1 2 7000
		./zad3_shared 1000 1500 0 0 7000
		./zad3_shared 1000 1500 1 0 7000
		./zad3_shared 1000 1500 0 1 7000
		./zad3_shared 1000 1500 1 1 7000
		./zad3_shared 1000 1500 0 2 7000
		./zad3_shared 1000 1500 1 2 7000

		./zad3_shared 60000 1200 0 0 15000
		./zad3_shared 60000 1200 1 0 15000
		./zad3_shared 60000 1200 0 1 15000
		./zad3_shared 60000 1200 1 1 15000
		./zad3_shared 60000 1200 0 2 15000
		./zad3_shared 60000 1200 1 2 15000
		./zad3_static 60000 1200 1 1 15000
		./zad3_static 60000 1200 1 1 15000
		./zad3_static 60000 1200 1 1 15000
		./zad3_static 60000 1200 1 1 15000
		./zad3_static 60000 1200 1 2 15000
		./zad3_static 60000 1200 1 2 15000
		
		
		
		make clean
				
